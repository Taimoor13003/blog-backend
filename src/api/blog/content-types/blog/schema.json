{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "name": "blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "blogs"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "blogs"
    },
    "shortText": {
      "type": "text"
    },
    "CreateAtDate": {
      "type": "date"
    },
    "readingTime": {
      "type": "string",
      "default": "2 min",
      "required": true
    },
    "longDescription": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "type": "media",
      "allowedTypes": [
        "images"
      ],
      "multiple": false,
      "required": true
    },
    "author": {
      "type": "component",
      "repeatable": false,
      "component": "author.info",
      "required": true
    },
    "youtubeVideoId": {
      "type": "string"
    },
    "seoData": {
      "type": "component",
      "repeatable": false,
      "component": "seo-data.seo-data"
    },
    "preventIndexing": {
      "type": "boolean",
      "default": false
    },
    "keywords": {
      "type": "string",
      "required": true
    },
    "RelatedArticles": {
      "type": "component",
      "repeatable": false,
      "component": "related-articles.related-articles"
    }
  }
}
