{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blogs",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "category": {
      "model": "categories",
      "via": "blogs"
    },
    "tags": {
      "collection": "tags",
      "via": "blogs",
      "dominant": true
    },
    "shortText": {
      "type": "text"
    },
    "CreateAtDate": {
      "type": "date"
    },
    "readingTime": {
      "type": "string",
      "default": "2 min",
      "required": true
    },
    "longDescription": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "author": {
      "type": "component",
      "repeatable": false,
      "component": "author.info",
      "required": true
    },
    "youtubeVideoId": {
      "type": "string"
    },
    "seoData": {
      "type": "component",
      "repeatable": false,
      "component": "seo-data.seo-data"
    }
  }
}
